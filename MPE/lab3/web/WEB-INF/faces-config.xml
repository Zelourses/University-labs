<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>mainBean</managed-bean-name>
        <managed-bean-class>beans.MainBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
            <property-name>cb</property-name>
            <value>#{collectionBean}</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>collectionBean</managed-bean-name>
        <managed-bean-class>beans.CollectionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>


</faces-config>
